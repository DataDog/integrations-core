        {
            "id": 5856991256503130,
            "definition": {
                "title": "Top 10 Source of Drop Rate Count",
                "title_size": "16",
                "title_align": "left",
                "type": "toplist",
                "requests": [
                    {
                        "formulas": [
                            {
                                "formula": "query1",
                                "limit": {
                                    "count": 10,
                                    "order": "desc"
                                }
                            }
                        ],
                        "queries": [
                            {
                                "data_source": "logs",
                                "name": "query1",
                                "indexes": [
                                    "*"
                                ],
                                "compute": {
                                    "aggregation": "count"
                                },
                                "group_by": [
                                    {
                                        "facet": "@drop_object",
                                        "limit": 10,
                                        "sort": {
                                            "order": "desc",
                                            "aggregation": "count"
                                        }
                                    }
                                ],
                                "search": {
                                    "query": "source:cisco-secure-firewall @message_id:733100 $Device-ID $Object-IP-Address $Host"
                                },
                                "storage": "hot"
                            }
                        ],
                        "response_format": "scalar"
                    }
                ],
                "style": {
                    "display": {
                        "type": "stacked",
                        "legend": "automatic"
                    },
                    "scaling": "relative"
                }
            },
            "layout": {
                "x": 0,
                "y": 2,
                "width": 4,
                "height": 3
            }
        },
