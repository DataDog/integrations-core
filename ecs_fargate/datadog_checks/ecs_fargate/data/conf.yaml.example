init_config:
  ## @param skip_proxy - boolean - optional - default: false
  ## Set `skip_proxy` parameter to `true` bypass any proxy settings
  ## settings enabled.
  #
  #  skip_proxy: false

  ## @param proxy - list of string - optional
  ## Set HTTP or HTTPS proxy for all instances.
  ## Use the no_proxy list to specify hosts that must bypass proxy
  #  
  # proxy:
  #    https: https://<SSL_PROXY_SERVER_FOR_HTTPS>:<PORT>
  #    http: https://<SSL_PROXY_SERVER_FOR_HTTP>:<PORT>
  #    no_proxy:
  #      - <HOSTNAME_1>
  #      - <HOSTNAME_2>

instances:

  -

    ## @param timeout - integer - optional - default: 5
    ## Time to wait on the Fargate metadata API before timeout in second
    #
    # timeout: 5

    ## @param tags - list of key:value element - optional
    ## List of tags to attach to every metric, event and service check emitted by this integration.
    ##
    ## Learn more about tagging: https://docs.datadoghq.com/tagging/
    #
    # tags:
    #   - <KEY_1>:<VALUE_1>
    #   - <KEY_2>:<VALUE_2>

    ## @param label_whitelist - list of string - optional
    ## Whitelist containers label if you want to collect their corresponding metrics
    #
    # label_whitelist:
    #   - <LABEL>

    ## @param skip_proxy - boolean - optional - default: false
    ## Set `skip_proxy` parameter to `true` bypass any proxy settings
    ## settings enabled.
    #
    #  skip_proxy: false

    ## @param proxy - list of string - optional
    ## Set HTTP or HTTPS proxy for all instances.
    ## Use the no_proxy list to specify hosts that must bypass proxy
    #  
    # proxy:
    #    https: https://<SSL_PROXY_SERVER_FOR_HTTPS>:<PORT>
    #    http: https://<SSL_PROXY_SERVER_FOR_HTTP>:<PORT>
    #    no_proxy:
    #      - <HOSTNAME_1>
    #      - <HOSTNAME_2>
