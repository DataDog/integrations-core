        {
            "id": 6482357144316430,
            "definition": {
                "title": "Top 10 Clicked URLs",
                "title_size": "16",
                "title_align": "left",
                "type": "toplist",
                "requests": [
                    {
                        "queries": [
                            {
                                "data_source": "logs",
                                "name": "query1",
                                "indexes": [
                                    "*"
                                ],
                                "compute": {
                                    "aggregation": "count"
                                },
                                "group_by": [
                                    {
                                        "facet": "@trend_micro_email_security.url",
                                        "limit": 10,
                                        "sort": {
                                            "order": "desc",
                                            "aggregation": "count"
                                        }
                                    }
                                ],
                                "search": {
                                    "query": "source:trend-micro-email-security service:ctp_detection $Sender $Recipient $Severity $Action"
                                },
                                "storage": "hot"
                            }
                        ],
                        "response_format": "scalar",
                        "formulas": [
                            {
                                "formula": "query1"
                            }
                        ],
                        "sort": {
                            "count": 10,
                            "order_by": [
                                {
                                    "type": "formula",
                                    "index": 0,
                                    "order": "desc"
                                }
                            ]
                        }
                    }
                ],
                "style": {
