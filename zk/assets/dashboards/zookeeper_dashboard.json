        {
            "id": 12,
            "definition": {
                "type": "query_value",
                "requests": [
                    {
                        "q": "sum:zookeeper.pending_syncs{*}",
                        "aggregator": "avg",
                        "conditional_formats": [
                            {
                                "comparator": ">",
                                "value": 10,
                                "palette": "white_on_red"
                            },
                            {
                                "comparator": "<",
                                "value": 10,
                                "palette": "white_on_green"
                            }
                        ]
                    }
                ],
                "custom_links": [],
                "title": "Pending syncs",
                "title_size": "16",
                "title_align": "center",
                "time": {
                    "live_span": "1h"
                },
                "precision": 0
            },
            "layout": {
                "x": 0,
                "y": 15,
                "width": 18,
                "height": 7
            }
        },
        {
            "id": 13,
            "definition": {
                "type": "query_value",
                "requests": [
                    {
                        "q": "( ( sum:zookeeper.max_file_descriptor_count{*} - sum:zookeeper.open_file_descriptor_count{*} ) / sum:zookeeper.max_file_descriptor_count{*} ) * 100",
                        "aggregator": "avg",
                        "conditional_formats": [
                            {
                                "comparator": "<=",
                                "value": 15,
                                "palette": "white_on_red"
                            },
                            {
                                "comparator": ">=",
                                "value": 25,
                                "palette": "white_on_green"
                            },
                            {
                                "comparator": ">",
                                "value": 16,
                                "palette": "white_on_yellow"
                            }
                        ]
                    }
                ],
                "custom_links": [],
                "title": "Available % FD",
                "title_size": "16",
                "title_align": "left",
                "time": {
                    "live_span": "1h"
                },
                "precision": 2
            },
            "layout": {
                "x": 0,
                "y": 39,
                "width": 18,
                "height": 7
            }
        },
