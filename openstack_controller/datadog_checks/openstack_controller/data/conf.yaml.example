init_config:
      ## @param keystone_server_url - string - required
      ## URL of your identity server. Note that the server must support Identity API v3
      #
      keystone_server_url: <"https://my-keystone-server.com:<port>/">


      ## @param os_host - string - optional
      ## The hostname of this machine registered with Nova. 
      ## Defaults to the Hostname detected by the Agent - https://docs.datadoghq.com/agent/faq/how-datadog-agent-determines-the-hostname/
      #
      # os_host: my_hostname


      ## @param whitelist_project_names - list - optional - default:[]
      ## IDs of projects to whitelist for monitoring (by default the agent will collect limit metrics from all projects)
      ## Regex expressions for the project names are supported.
      ## Whitlist takes precedence over blacklist in case of overlap
      #
      # whitelist_project_names:
      #    - proj1
      #    - prod_*


      ## @param blacklist_project_names - list - optional - default:[]
      ## IDs of projects to blacklist for monitoring (by default the agent will collect limit metrics from all projects)
      ## Regex expressions for the project names are supported.
      ## Whitelist takes precedence over blacklist in case of overlap
      #
      # blacklist_project_names:
      #    - proj2
      #    - test_*


      ## @param exclude_server_ids - list - optional - default:[]
      ## IDs of servers to exclude from monitoring. (by default the agent will collect metrics from all guest servers)
      ## Regex expressions for the server IDs are supported.
      #
      # exclude_server_ids:
      #    - server_1
      #    - other_.*


      ## @param exclude_network_ids - optional - list - default:[]
      ## IDs of networks to exclude from monitoring (by default the agent will collect metrics from networks returned by the neutron:get_networks operation)
      ## Regex expressions for the network IDs to exclude are supported.
      #
      # exclude_network_ids:
      #    - network_1
      #    - other_.*

      ## @param nova_api_version - optional - string - default:'v2.1'
      ## Nova API version to use - this check supports v2 and v2.1 (default)
      #
      # nova_api_version: 'v2.1'

      ## @param ssl_verify - optional - bool - default:true
      ## Whether to enable SSL certificate verification for HTTP requests. Defaults to true, you may
      ## need to set to false when using self-signed certs
      #
      # ssl_verify: true

     ## @param use_agent_proxy - optional - bool - default:true
     ## Whether the dd-agent proxy should also be used for openstack API requests (if set)
     #
     # use_agent_proxy: true

instances:
    ## @param name - required - string
    ## Unique identifier for this instance
    #
    - name: instance_1


      ## @param user - required - user_object
      ## Password authentication is the only auth method supported
      ## User expects username, password, and user domain id
      ## `user` should resolve to a structure like {'password': 'my_password', 'name': 'my_name', 'domain': {'id': 'my_domain_id'}}
      ## The check will use the Unscoped token method to collect information about all available projects to the user
      #
      user:
          password: <my_password>
          name: <datadog>
          domain:
              id: <default>


      ## @param append_tenant_id - optional - string - default:false
      ## In some cases, the nova url is returned without the tenant id suffixed
      ## e.g. http://172.0.0.1:8774 rather than http://172.0.0.1:8774/<tenant_id>
      ## Setting append_tenant_id to true manually adds this suffix for downstream requests
      #
      # append_tenant_id: false


      ## @param collect_hypervisor_load - optional - bool - default:false
      ## Collects hypervisor_load.1/5/15 for each hypervisor
      ## Disabled by default to increase performance of the check.
      ## With this enabled there is an extra N requests per check run (N = # of hypervisors)
      ## If the Agent is installed on each hypervisor, this metric is available as system.load.1/5/15
      #
      # collect_hypervisor_load: False


      ## @param use_shortname - optional - bool - default:false
      ## In some OpenStack environments, the hostname registered to Nova is the shortname. 
      ## Enabling this will enforce the check to split the hostname up to the first period when
      ## comparing against nova responses. Only affects aggregate tagging. 
      #
      # use_shortname: False


      ## @param collect_limits_from_all_projects - optional - bool - default:True
      ## The admin user defined for Datadog will start with a "default" project. 
      ## If this option is disabled, project limits metrics will only be collected from projects the
      ## datadog defined user has access to view. 
      #
      # collect_limits_from_all_projects: True

      
      ## @param tags - optional - list - default:[]
      ## Please note that server metrics override the host tag and thus do not get
      ## the host-level tags specified in datadog.(conf/yaml)
      ## To apply tags to all openstack metrics, you can use the following "tags" option 
      ##
      ## Set (optional) custom tags for each metric
      #
      # tags:
      #   - optional:tag1

