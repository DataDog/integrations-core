[
    {
        "agent_version": "5.7.0",
        "integration": "StatsD",
        "groups": [
            "host",
            "port"
        ],
        "check": "statsd.can_connect",
        "statuses": [
            "ok",
            "critical"
        ],
        "name": "Can Connect",
        "description": "Returns Critical if the Agent cannot send data to the StatsD endpoint, OK otherwise."
    },
    {
        "agent_version": "5.7.0",
        "integration": "StatsD",
        "groups": [
            "host",
            "port"
        ],
        "check": "statsd.is_up",
        "statuses": [
            "ok",
            "critical"
        ],
        "name": "Is Up",
        "description": "Returns Critical if the Agent cannot connect to the StatsD url, OK otherwise."
    }
]
