                            {
                                "comparator": ">=",
                                "value": 80,
                                "palette": "yellow_on_white"
                            },
                            {
                                "comparator": "<",
                                "value": 80,
                                "palette": "red_on_white"
                            }
                        ]
                    }
                ],
                "custom_links": [],
