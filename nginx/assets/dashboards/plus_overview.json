    {
      "id": 8519992001981460,
      "definition": {
        "title": "SSL",
        "type": "group",
        "background_color": "vivid_green",
        "show_title": true,
        "layout_type": "ordered",
        "widgets": [
          {
            "id": 4252730268672606,
            "definition": {
              "title": "Handshakes per hosts",
              "title_size": "16",
              "title_align": "left",
              "type": "query_table",
              "requests": [
                {
                  "q": "sum:nginx.ssl.handshakes_count{*} by {host}.as_count()",
                  "alias": "Successful",
                  "limit": 10,
                  "order": "desc",
                  "aggregator": "sum"
