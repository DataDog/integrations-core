                  "q": "top(avg:snmp.ifInErrors{$snmp_host,$interface} by {snmp_host,interface}.as_count()+avg:snmp.ifOutErrors{$snmp_host,$interface} by {snmp_host,interface}.as_count(),5,'mean','desc')"
