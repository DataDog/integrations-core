[
    {
        "agent_version": "5.5.0",
        "integration": "Consul",
        "groups": [
            "host",
            "check",
            "service",
            "consul_service_id"
        ],
        "check": "consul.check",
        "statuses": [
            "ok",
            "warning",
            "critical",
            "unknown"
        ],
        "name": "Health Check",
        "description": "Returns OK if the service is up, WARNING if there is an issue and CRITICAL when down."
    },
    {
        "agent_version": "5.5.0",
        "integration": "Consul",
        "groups": [
            "host",
            "consul_url"
        ],
        "check": "consul.up",
        "statuses": [
            "ok",
            "critical"
        ],
        "name": "Up",
        "description": "Returns OK if the consul server is up, CRITICAL otherwise."
    },
    {
        "agent_version": "5.5.0",
        "integration": "Consul",
        "groups": [
            "host",
            "url"
        ],
        "check": "consul.can_connect",
        "statuses": [
            "ok",
            "critical"
        ],
        "name": "Can Connect",
        "description": "Returns OK if the Agent can make HTTP requests to consul, CRITICAL otherwise."
    },
    {
        "agent_version": "7.23.0",
        "integration": "Consul",
        "groups": [
            "host",
            "endpoint"
        ],
        "check": "consul.prometheus.health",
        "statuses": [
            "ok",
            "critical"
        ],
        "name": "Consul Prometheus health",
        "description": "Returns `CRITICAL` if the check cannot access the metrics endpoint, otherwise returns `OK`."
    }
]
