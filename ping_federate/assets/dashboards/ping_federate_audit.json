{
  "description": "This dashboard provides information about the Security Audit logs generated in PingFederate server.",
  "layout_type": "ordered",
  "notify_list": [
  ],
  "reflow_type": "fixed",
  "template_variables": [
    {
      "available_values": [
        "AUTHN_ATTEMPT",
        "SLO",
        "SSO"
      ],
      "default": "*",
      "name": "Event_Name",
      "prefix": "@evt.name"
    },
    {
      "available_values": [
        "success",
        "failure",
        "inprogress"
      ],
      "default": "*",
      "name": "Status",
      "prefix": "@evt.outcome"
    },
    {
      "available_values": [
        "IdP",
        "SP"
      ],
      "default": "*",
      "name": "Role",
      "prefix": "@ping_federate.transaction.role"
    }
  ],
