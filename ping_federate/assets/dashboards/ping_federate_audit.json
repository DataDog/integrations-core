    {
      "id": 1325464047476426,
      "definition": {
        "title": "Event Details",
        "title_size": "16",
        "title_align": "left",
        "requests": [
          {
            "response_format": "event_list",
            "query": {
              "data_source": "logs_stream",
              "query_string": "source:ping-federate service:audit $event_name $status $role ",
              "indexes": [],
              "storage": "hot",
              "sort": { "order": "desc", "column": "timestamp" }
            },
            "columns": [
              { "field": "status_line", "width": "auto" },
              { "field": "timestamp", "width": "auto" },
              { "field": "host", "width": "auto" },
              { "field": "@evt.name", "width": "auto" },
              { "field": "@ping_federate.transaction.role", "width": "auto" },
              { "field": "@evt.outcome", "width": "auto" },
              { "field": "@ping_federate.transaction.subject", "width": "auto" }
            ]
          }
        ],
        "type": "list_stream"
      },
      "layout": { "x": 0, "y": 28, "width": 12, "height": 4 }
    }
  ],
  "template_variables": [
    {
      "name": "event_name",
      "prefix": "@evt.name",
      "available_values": ["AUTHN_ATTEMPT", "SLO", "SSO"],
      "default": "*"
    },
    {
      "name": "role",
      "prefix": "@transaction.role",
      "available_values": ["IdP", "SP"],
      "default": "*"
    },
    {
      "name": "status",
      "prefix": "@evt.outcome",
      "available_values": ["failure", "inprogress", "success"],
      "default": "*"
    }
  ],
  "layout_type": "ordered",
