    {
      "id": 4406677250259292,
      "definition": {
        "title": "Total Transactions",
        "title_size": "16",
        "title_align": "left",
        "type": "query_value",
        "requests": [
          {
            "response_format": "scalar",
            "queries": [
              {
                "data_source": "logs",
                "name": "query1",
                "indexes": ["*"],
                "compute": { "aggregation": "count" },
                "group_by": [],
                "search": {
                  "query": "source:ping-federate service:audit $event_name $status $role"
                },
                "storage": "hot"
              }
            ],
            "formulas": [{ "formula": "query1" }],
            "conditional_formats": [
              {
                "comparator": ">",
                "value": 0,
                "palette": "custom_bg",
                "custom_bg_color": "#dbdef5"
              }
            ]
          }
        ],
        "autoscale": true,
        "precision": 2,
        "timeseries_background": {
          "type": "area",
          "yaxis": { "include_zero": true }
        }
      },
      "layout": { "x": 0, "y": 2, "width": 12, "height": 3 }
    },
