    {
      "id": 3081339105298010,
      "definition": {
        "title": "Distribution by Role",
        "title_size": "16",
        "title_align": "left",
        "requests": [
          {
            "response_format": "scalar",
            "formulas": [{ "formula": "query1" }],
            "queries": [
              {
                "data_source": "logs",
                "name": "query1",
                "indexes": ["*"],
                "compute": { "aggregation": "count" },
                "group_by": [
                  {
                    "facet": "@ping_federate.transaction.role",
                    "limit": 10,
                    "sort": { "order": "desc", "aggregation": "count" }
                  }
                ],
                "search": {
                  "query": "source:ping-federate service:audit $event_name $status $role"
                },
                "storage": "hot"
