       {
          "id":8455036962008856,
          "definition":{
             "title":"Overview",
             "background_color":"vivid_orange",
             "show_title":true,
             "type":"group",
             "layout_type":"ordered",
             "widgets":[
                {
                   "id":8079463037806680,
                   "definition":{
                      "title":"Failover Cluster Instances",
                      "title_size":"16",
                      "title_align":"left",
                      "type":"query_table",
                      "requests":[
                         {
                            "formulas":[
                               {
                                  "alias":"is owner",
                                  "limit":{
                                     "order":"desc"
                                  },
                                  "formula":"query1"
                               }
                            ],
                            "response_format":"scalar",
                            "queries":[
                               {
                                  "query":"sum:sqlserver.fci.is_current_owner{$host} by {host,node_name,failover_cluster,status}",
                                  "data_source":"metrics",
                                  "name":"query1",
                                  "aggregator":"avg"
                               }
                            ]
                         }
                      ]
                   },
                   "layout":{
                      "x":0,
                      "y":0,
                      "width":7,
                      "height":2
                   }
                },
                {
                   "id":5833233245279158,
                   "definition":{
                      "title":"Availability Group Instances",
                      "title_size":"16",
                      "title_align":"left",
                      "type":"query_table",
                      "requests":[
                         {
                            "formulas":[
                               {
                                  "alias":"Num of DB",
                                  "limit":{
                                     "order":"desc"
                                  },
                                  "formula":"query4"
                               },
                               {
                                  "alias":"Primaries",
                                  "formula":"query1"
                               },
                               {
                                  "alias":"Secondaries",
                                  "formula":"query2"
                               },
                               {
                                  "alias":"Lag/s",
                                  "formula":"query3"
                               }
                            ],
                            "response_format":"scalar",
                            "queries":[
                               {
                                  "query":"sum:sqlserver.ao.replica_status{$host,$availability_group,$replica_server} by {host}",
                                  "data_source":"metrics",
                                  "name":"query4",
                                  "aggregator":"last"
                               },
                               {
                                  "query":"sum:sqlserver.ao.replica_status{$host,$availability_group,$replica_server,replica_role:primary} by {host}",
                                  "data_source":"metrics",
                                  "name":"query1",
                                  "aggregator":"last"
                               },
                               {
                                  "query":"sum:sqlserver.ao.replica_status{$host,$availability_group,$replica_server,replica_role:secondary} by {host}",
                                  "data_source":"metrics",
                                  "name":"query2",
                                  "aggregator":"last"
                               },
                               {
                                  "query":"sum:sqlserver.ao.secondary_lag_seconds{$host,$availability_group,$replica_server} by {host}",
                                  "data_source":"metrics",
                                  "name":"query3",
                                  "aggregator":"last"
                               }
                            ]
                         }
                      ]
                   },
                   "layout":{
                      "x":0,
                      "y":2,
                      "width":7,
                      "height":2
                   }
                },
                {
                   "id":6329887077952744,
                   "definition":{
                      "title":"Availability Groups",
                      "title_size":"16",
                      "title_align":"left",
                      "type":"query_table",
                      "requests":[
                         {
                            "formulas":[
                               {
                                  "alias":"is primary",
                                  "cell_display_mode":"number",
                                  "limit":{
                                     "order":"desc"
                                  },
                                  "conditional_formats":[
                                     {
                                        "palette":"white_on_green",
                                        "comparator":">=",
                                        "value":1
                                     },
                                     {
                                        "palette":"white_on_yellow",
                                        "custom_bg_color":"#00fcff",
                                        "value":0,
                                        "comparator":"<="
                                     }
                                  ],
                                  "formula":"query1"
                               },
                               {
                                  "alias":"lag/s",
                                  "formula":"query2"
                               }
                            ],
                            "response_format":"scalar",
                            "queries":[
                               {
                                  "query":"max:sqlserver.ao.is_primary_replica{$host,$availability_group,$replica_server} by {availability_group_name,replica_server_name,database_name,synchronization_state,failover_mode}",
                                  "data_source":"metrics",
                                  "name":"query1",
                                  "aggregator":"last"
                               },
                               {
                                  "query":"sum:sqlserver.ao.secondary_lag_seconds{$host,$availability_group,$replica_server} by {availability_group_name,replica_server_name,database_name,synchronization_state,failover_mode}",
                                  "data_source":"metrics",
                                  "name":"query2",
                                  "aggregator":"last"
                               }
                            ]
                         }
                      ]
                   },
                   "layout":{
                      "x":0,
                      "y":4,
                      "width":7,
                      "height":3
                   }
                }
             ]
          },
          "layout":{
             "x":5,
             "y":0,
             "width":7,
             "height":8
          }
