              "requests": [
                {
                  "aggregator": "avg",
                  "cell_display_mode": [
                    "bar"
                  ],
                  "q": "avg:postgresql.total_size{$scope,$db,$table} by {table}",
                  "alias": "Disk Usage",
                  "limit": 50,
                  "order": "desc"
                },
                {
                  "q": "avg:postgresql.index_scans{$scope,$db,$table} by {table}",
                  "alias": "Index Scans",
