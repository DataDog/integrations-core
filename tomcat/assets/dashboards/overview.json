      {
        "id": 1051748789667920,
        "definition": {
          "title": "Overview",
          "title_align": "center",
          "type": "group",
          "background_color": "vivid_purple",
          "layout_type": "ordered",
          "widgets": [
            {
              "id": 6207700556953864,
              "definition": {
                "title": "Server can connect",
                "title_size": "16",
                "title_align": "center",
                "type": "check_status",
                "check": "tomcat.can_connect",
                "grouping": "cluster",
                "group": "$host",
                "group_by": ["host"],
                "tags": ["$host"]
              },
              "layout": { "x": 0, "y": 0, "width": 2, "height": 2 }
            },
            {
              "id": 4926734957335396,
              "definition": {
                "title": "% of max heap",
                "title_size": "16",
                "title_align": "left",
                "type": "query_value",
                "requests": [
                  {
                    "q": "(sum:jvm.heap_memory{*}/sum:jvm.heap_memory_max{*})*100",
                    "aggregator": "last",
                    "conditional_formats": [
                      {
                        "comparator": ">",
                        "palette": "white_on_red",
                        "value": 80
                      },
                      {
                        "comparator": ">=",
                        "palette": "white_on_yellow",
                        "value": 51
                      },
                      {
                        "comparator": "<",
                        "palette": "white_on_green",
                        "value": 50
                      }
                    ]
                  }
                ],
                "autoscale": true,
                "custom_unit": "%",
                "precision": 0
              },
              "layout": { "x": 2, "y": 0, "width": 2, "height": 2 }
            },
            {
              "id": 5833900934884076,
              "definition": {
                "title": "% of max threads",
                "title_size": "16",
                "title_align": "left",
                "type": "query_value",
                "requests": [
                  {
                    "q": "(sum:tomcat.threads.busy{*}/sum:tomcat.threads.max{*})*100",
                    "aggregator": "last",
                    "conditional_formats": [
                      {
                        "comparator": ">",
                        "palette": "white_on_red",
                        "value": 80
                      },
                      {
                        "comparator": ">=",
                        "palette": "white_on_yellow",
                        "value": 50
                      },
                      {
                        "comparator": "<",
                        "palette": "white_on_green",
                        "value": 49
                      }
                    ]
                  }
                ],
                "autoscale": true,
                "custom_unit": "%",
                "precision": 0
              },
              "layout": { "x": 0, "y": 2, "width": 2, "height": 2 }
            },
            {
              "id": 1941945703480454,
              "definition": {
                "type": "note",
                "content": "Percentage of threads in use calculated from the set max threads.\n\n沐ｴ **Red** if > 80%\n\n沺｡ **Yellow** if >= 50%\n\n沺｢ **Green** if < 49%\n",
                "background_color": "white",
                "font_size": "12",
                "text_align": "left",
                "vertical_align": "top",
                "show_tick": true,
                "tick_pos": "50%",
                "tick_edge": "left",
                "has_padding": true
              },
              "layout": { "x": 2, "y": 2, "width": 2, "height": 2 }
            }
          ]
        },
        "layout": { "x": 8, "y": 0, "width": 4, "height": 5 }
      },
