            "id": 4926734957335396,
            "definition": {
              "title": "% of max heap",
              "title_size": "16",
              "title_align": "left",
              "type": "query_value",
              "requests": [
                {
                  "conditional_formats": [
                    {
                      "comparator": ">",
                      "palette": "white_on_red",
                      "value": 80
                    },
                    {
                      "comparator": ">=",
                      "palette": "white_on_yellow",
                      "value": 51
                    },
                    {
                      "comparator": "<",
                      "palette": "white_on_green",
                      "value": 50
                    }
                  ],
                  "response_format": "scalar",
                  "queries": [
                    {
                      "data_source": "metrics",
                      "name": "query1",
                      "query": "sum:jvm.heap_memory{*}",
                      "aggregator": "last"
                    },
                    {
                      "data_source": "metrics",
                      "name": "query2",
                      "query": "sum:jvm.heap_memory_max{*}",
                      "aggregator": "last"
                    }
                  ],
                  "formulas": [
                    {
                      "formula": "(query1 / query2) * 100"
                    }
                  ]
                }
              ],
              "autoscale": true,
              "custom_unit": "%",
              "precision": 0
            },
            "layout": {
              "x": 3,
