{
	"id": 4722049,
	"name": "[Databricks Model Serving] 4xx Errors higher than usual for endpoint: {{endpoint_name.name}}",
	"type": "query alert",
	"query": "avg(last_4h):anomalies(avg:databricks.model_serving.request_4xx_count_total{*} by {endpoint_name}, 'basic', 2, direction='both', interval=60, alert_window='last_15m', count_default_zero='true') >= 1",
	"message": "The number of 4xx errors for Databricks Model Serving endpoint: {{endpoint_name.name}} is at {{value}}, which is higher than usual.",
	"tags": [
		"integration:databricks"
	],
	"options": {
		"thresholds": {
			"critical": 1,
			"critical_recovery": 0
		},
		"notify_audit": false,
		"require_full_window": false,
		"renotify_interval": 0,
		"threshold_windows": {
			"trigger_window": "last_15m",
			"recovery_window": "last_15m"
		},
		"on_missing_data": "default",
		"include_tags": true,
		"new_group_delay": 60,
		"avalanche_window": 10
	},
	"priority": null,
	"restriction_policy": {
		"bindings": []
	}
}