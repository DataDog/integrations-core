            "id": 4806917799331273,
            "definition": {
                "title": "Total Detections",
                "title_size": "16",
                "title_align": "left",
                "type": "query_value",
                "requests": [
                    {
                        "response_format": "scalar",
                        "queries": [
                            {
                                "name": "query1",
                                "data_source": "logs",
                                "search": {
                                    "query": "source:kandji service:detection $device_name $blueprint_name $cve_id $file_name $device_model $severity"
                                },
                                "indexes": [
                                    "*"
                                ],
                                "group_by": [],
                                "compute": {
                                    "aggregation": "count"
                                },
                                "storage": "hot"
                            }
                        ],
                        "formulas": [
                            {
                                "formula": "query1"
                            }
                        ],
                        "conditional_formats": [
                            {
                                "comparator": ">",
