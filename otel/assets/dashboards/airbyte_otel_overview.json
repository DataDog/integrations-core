{
  "title": "Airbyte OTel Overview",
  "description": "**Airbyte OTel**\n\nThis dashboard monitors your Airbyte deployment using OpenTelemetry metrics. All queries use OTel metric names and tags as per https://docs.airbyte.com/platform/operator-guides/collecting-metrics#available-metrics\n\n**Useful Links**\n\n* [Airbyte OTel Metrics](https://docs.airbyte.com/platform/operator-guides/collecting-metrics#available-metrics)",
  "layout_type": "ordered",
  "widgets": [
    {
      "id": 1,
      "definition": {
        "type": "note",
        "content": "This dashboard uses OTel metrics: airbyte.syncs, airbyte.sync_duration, airbyte.gb_moved, etc. All queries use OTel tags such as status, connection_id, workspace_id, etc.",
        "background_color": "white",
        "font_size": "14",
        "text_align": "left",
        "vertical_align": "top",
        "show_tick": false,
        "has_padding": true
      },
      "layout": {"x": 0, "y": 0, "width": 6, "height": 2}
    },
    {
      "id": 2,
      "definition": {
        "title": "Syncs by Status",
        "type": "timeseries",
        "requests": [
          {
            "q": "sum:airbyte.syncs{*} by {status}"
          }
        ]
      },
      "layout": {"x": 0, "y": 2, "width": 12, "height": 4}
    },
    {
      "id": 3,
      "definition": {
        "title": "Average Sync Duration (Seconds)",
        "type": "timeseries",
        "requests": [
          {
            "q": "avg:airbyte.sync_duration{*}"
          }
        ]
      },
      "layout": {"x": 0, "y": 6, "width": 12, "height": 4}
    },
    {
      "id": 4,
      "definition": {
        "title": "Total Data Volume Moved (GB)",
        "type": "timeseries",
        "requests": [
          {
            "q": "sum:airbyte.gb_moved{*}"
          }
        ]
      },
      "layout": {"x": 0, "y": 10, "width": 12, "height": 4}
    },
    {
      "id": 5,
      "definition": {
        "title": "Syncs per Connection",
        "type": "timeseries",
        "requests": [
          {
            "q": "sum:airbyte.syncs{*} by {connection_id}"
          }
        ]
      },
      "layout": {"x": 0, "y": 14, "width": 12, "height": 4}
    }
  ],
  "notify_list": [],
  "reflow_type": "fixed"
}
