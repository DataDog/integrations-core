      {
        "id": 8142057437817140,
        "definition": {
          "title": "Cluster - All Master Objects",
          "title_size": "16",
          "title_align": "left",
          "type": "query_value",
          "requests": [
            {
              "response_format": "scalar",
              "queries": [
                {
                  "data_source": "metrics",
                  "name": "query1",
                  "query": "sum:aerospike.aerospike_namespace_master_objects{$aerospike_cluster}",
                  "aggregator": "last"
                }
              ],
              "conditional_formats": [
                {
                  "comparator": ">=",
                  "value": 0,
                  "palette": "white_on_green"
                }
              ],
              "formulas": [
                {
                  "formula": "query1"
                }
              ]
            }
          ],
          "autoscale": false,
          "precision": 0,
          "timeseries_background": {
            "type": "bars"
          }
        },
        "layout": {
          "x": 0,
          "y": 1,
          "width": 3,
          "height": 2
        }
      },
      {
        "id": 6803820697630468,
        "definition": {
          "title": "Cluster- Unique Data Bytes",
          "title_size": "16",
          "title_align": "left",
          "type": "query_value",
          "requests": [
            {
              "response_format": "scalar",
              "queries": [
                {
                  "data_source": "metrics",
                  "name": "query1",
                  "query": "sum:aerospike.aerospike_namespace_data_used_bytes{$aerospike_cluster}",
                  "aggregator": "last"
                },
                {
                  "data_source": "metrics",
                  "name": "query3",
                  "query": "avg:aerospike.aerospike_namespace_effective_replication_factor{$aerospike_cluster}",
                  "aggregator": "last"
                },
                {
                  "data_source": "metrics",
                  "name": "query2",
                  "query": "avg:aerospike.aerospike_namespace_data_compression_ratio{$aerospike_cluster}",
                  "aggregator": "last"
                },
                {
                  "data_source": "metrics",
                  "name": "query4",
                  "query": "sum:aerospike.aerospike_namespace_master_objects{$aerospike_cluster}",
                  "aggregator": "last"
                }
              ],
              "conditional_formats": [
                {
                  "comparator": ">=",
                  "value": 0,
                  "palette": "white_on_green"
                }
              ],
              "formulas": [
                {
                  "formula": "((query1 / query3) / query2) - (query4 * 39)"
                }
              ]
            }
          ],
          "autoscale": false,
          "precision": 0,
          "timeseries_background": {
            "type": "area"
          }
        },
        "layout": {
