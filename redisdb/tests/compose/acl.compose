version: '3.5'

services:
  redis-acl:
    image: "redis:${REDIS_VERSION}"
    volumes:
      - ${REDIS_CONFIG}:/etc/redis.conf
      - ${REDIS_USERS_ACL}:/etc/users.acl
    ports:
      - "6383:6379"
    networks:
      - network1
    command: redis-server /etc/redis.conf --aclfile /etc/users.acl

networks:
  network1:
    name: redis_acl_default
