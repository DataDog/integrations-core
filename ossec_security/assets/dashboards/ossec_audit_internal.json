        {
            "id": 4237444399759780,
            "definition": {
                "title": "Total Count of Audit Events",
                "title_size": "16",
                "title_align": "left",
                "type": "query_value",
                "requests": [
                    {
                        "response_format": "scalar",
                        "queries": [
                            {
                                "data_source": "logs",
                                "name": "query1",
                                "indexes": [
                                    "*"
                                ],
                                "compute": {
                                    "aggregation": "count"
                                },
                                "group_by": [],
                                "search": {
                                    "query": "source:ossec-security service:system $Level $Group_List $Host_Name"
                                },
                                "storage": "hot"
                            }
                        ],
                        "formulas": [
                            {
                                "formula": "query1"
                            }
                        ]
                    }
                ],
                "autoscale": true,
                "precision": 2,
                "timeseries_background": {
                    "type": "area"
                }
            },
            "layout": {
                "x": 0,
                "y": 2,
                "width": 3,
                "height": 4
            }
        },
        {
            "id": 3647886025277710,
            "definition": {
                "title": "Top 10 Events by Level (Level >3)",
                "title_size": "16",
                "title_align": "left",
                "type": "toplist",
                "requests": [
                    {
                        "queries": [
                            {
                                "data_source": "logs",
                                "name": "query1",
                                "indexes": [
                                    "*"
                                ],
                                "compute": {
                                    "aggregation": "count"
                                },
                                "group_by": [
                                    {
                                        "facet": "@level",
                                        "limit": 10,
                                        "sort": {
                                            "order": "desc",
                                            "aggregation": "count"
                                        }
                                    }
                                ],
                                "search": {
                                    "query": "source:ossec-security service:system @level:>3 $Level $Group_List $Host_Name"
                                },
                                "storage": "hot"
                            }
                        ],
                        "response_format": "scalar",
                        "formulas": [
                            {
                                "formula": "query1"
                            }
