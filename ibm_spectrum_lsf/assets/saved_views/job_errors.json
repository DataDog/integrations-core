{
    "name": "IBM Spectrum LSF Job Error Logs",
    "type": "logs",
    "page": "stream",
    "query": "source:ibm_spectrum_lsf status:error log_type:job",
    "timerange": {
      "interval_ms": 3600000
    },
    "visible_facets": [
      "source",
      "host",
      "service",
      "@job_id"
    ],
    "options": {
      "columns": [
        "host",
        "service",
        "@job_id"
      ],
      "show_date_column": true,
      "show_message_column": true,
      "message_display": "inline",
      "show_timeline": true
    }
}
