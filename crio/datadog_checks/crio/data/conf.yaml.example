init_config:

instances:
    ## @param prometheus_url - string - required
    ## URL of the prometheus endpoint.
    #
  - prometheus_url: http://localhost:9090/metrics
    #
    ## @param tags  - list of key:value elements - optional
    ## List of tags to attach to every metrics, events and service checks emitted by this integration.
    ## Learn more about tagging: https://docs.datadoghq.com/tagging/
    #
    # tags:
    #   - <KEY_1>:<VALUE_1>,<KEY_2>:<VALUE_3>
    #
    ## @param prometheus_timeout - timeout in second - optional
    ## Overrides the default timeout value of 10 seconds
    #
    # prometheus_timeout: <VALUE_IN_SECOND>
