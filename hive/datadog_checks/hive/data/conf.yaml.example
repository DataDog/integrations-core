            metric_type: gauge
          Mean:
            alias: hive.server.api.sql_operation.pending.mean
            metric_type: gauge
          Min:
            alias: hive.server.api.sql_operation.pending.min
            metric_type: gauge
          75thPercentile:
            alias: hive.server.api.sql_operation.pending.75percentile
            metric_type: gauge
          95thPercentile:
