          MeanRate:
            alias: hive.server.api.operation.initialized.meanrate
            metric_type: rate

    # Pending
    - include:
        bean: metrics:name=active_calls_api_hs2_operation_PENDING
        attribute:
          Count:
            alias: hive.server.api.operation.pending.active_call
            metric_type: gauge

    - include:
