[
    {
        "agent_version": "6.0.0",
        "integration": "Kubernetes Controller Manager",
        "check": "kube_controller_manager.prometheus.health",
        "statuses": [
            "ok",
            "critical"
        ],
        "groups": [
            "endpoint"
        ],
        "name": "Kube Controller Manager prometheus health",
        "description": "Returns `CRITICAL` if the check cannot access the metrics endpoint."
    },
    {
        "agent_version": "6.0.0",
        "integration": "Kubernetes Controller Manager",
        "check": "kube_controller_manager.leader_election.status",
        "statuses": [
            "ok",
            "critical"
        ],
        "groups": [
            "record_kind",
            "record_name",
            "record_namespace"
        ],
        "name": "Kube Controller Manager leader election health",
        "description": "Returns `CRITICAL` if no replica is currently set as leader."
    },
    {
        "agent_version": "6.0.0",
        "integration": "Kubernetes Controller Manager",
        "check": "kube_controller_manager.up",
        "groups": [],
        "statuses": [
            "ok",
            "critical"
        ],
        "name": "Kube Controller Manager health",
        "description": "Returns `CRITICAL` if Kube Controller Manager is not healthy."
    }
]
