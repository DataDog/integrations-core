## This file is overwritten upon Agent upgrade.
## To make modifications to the check configuration, please copy this file
## to `agent_metrics.yaml` and make your changes on that file.

init_config:

    ## @param process_metrics - list of metrics - required
    ##
    #
    process_metrics:
        - name: memory_info
          type: gauge
          active: true
        - name: io_counters
          type: rate
          active: true
        - name: num_threads
          type: gauge
          active: true
        - name: connections
          type: gauge
          active: false

instances:

  -
    ## @param tags  - list of key:value strings - optional
    ## List of tags to attach to every metric and service check emitted by this integration.
    ##
    ## Learn more about tagging: https://docs.datadoghq.com/tagging/
    #
    # tags:
    #   - <KEY_1>:<VALUE_1>
    #   - <KEY_2>:<VALUE_2>
