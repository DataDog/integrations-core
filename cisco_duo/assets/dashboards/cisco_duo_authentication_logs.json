        {
            "id": 8243739200683380,
            "definition": {
                "title": "Total Successful Authentication",
                "title_size": "16",
                "title_align": "left",
                "type": "query_value",
                "requests": [
                    {
                        "response_format": "scalar",
                        "queries": [
                            {
                                "data_source": "logs",
                                "name": "query1",
                                "indexes": [
                                    "*"
                                ],
                                "compute": {
                                    "aggregation": "count"
                                },
                                "group_by": [],
                                "search": {
                                    "query": "source:cisco-duo service:authentication @result:success $Result $Application $Authentication-Method $User-Name"
                                },
                                "storage": "hot"
                            }
                        ],
                        "formulas": [
                            {
                                "formula": "query1"
                            }
                        ],
                        "conditional_formats": [
                            {
