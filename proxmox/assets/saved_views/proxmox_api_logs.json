{
  "name": "Proxmox API Logs",
  "type": "logs",
  "page": "stream",
  "query": "source:proxmox (service:pvedaemon OR service:pveproxy)",
  "timerange": {
    "interval_ms": 900000
  },
  "visible_facets": [
    "source",
    "host",
    "service"
  ],
  "options": {
    "columns": [
      "host",
      "service"
    ],
    "show_date_column": true,
    "show_message_column": true,
    "message_display": "inline",
    "show_timeline": true
  }
}