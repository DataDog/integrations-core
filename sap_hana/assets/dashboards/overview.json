        "definition": {
          "type": "query_value",
          "requests": [
            {
              "q": "avg:sap_hana.disk.utilized{*}",
              "aggregator": "avg",
              "conditional_formats": [
                {
                  "comparator": "<",
                  "value": 70,
                  "palette": "white_on_green"
                },
                {
                  "comparator": ">=",
                  "value": 70,
                  "palette": "white_on_yellow"
                },
                {
                  "comparator": ">=",
                  "value": 80,
                  "palette": "white_on_red"
                }
              ]
            }
          ],
          "custom_links": [],
          "title": "Disk utilization",
          "title_size": "16",
          "title_align": "left",
          "time": {
            "live_span": "15m"
          },
          "autoscale": true,
          "custom_unit": "%",
          "precision": 2
        },
        "layout": {
          "x": 1,
          "y": 14,
