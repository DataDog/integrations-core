[
    {
        "agent_version": "6.16.0",
        "integration": "SAP HANA",
        "groups": [
            "host",
            "server",
            "port"
        ],
        "check": "sap_hana.can_connect",
        "statuses": [
            "ok",
            "critical"
        ],
        "name": "Can Connect",
        "description": "Returns `CRITICAL` if the Agent is unable to connect to the monitored SAP HANA system, or `OK` otherwise."
    },
    {
        "agent_version": "6.16.0",
        "integration": "SAP HANA",
        "groups": [
            "host",
            "server",
            "port",
            "db"
        ],
        "check": "sap_hana.status",
        "statuses": [
            "ok",
            "critical"
        ],
        "name": "Database status",
        "description": "Returns `OK` if the monitored SAP HANA database is up, or `CRITICAL` otherwise."
    }
]
