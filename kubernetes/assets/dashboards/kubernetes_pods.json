                    "response_format": "timeseries",
                    "on_right_yaxis": false,
                    "queries": [
                      {
                        "query": "sum:kubernetes.pods.running{$scope,$cluster,$namespace,$deployment,$statefulset,$daemonset,$job,$cronjob}",
                        "data_source": "metrics",
                        "name": "query1"
