    {
      "id": 2258586113552824,
      "layout": {
        "x": 1,
        "y": 75,
        "width": 47,
        "height": 20
      },
      "definition": {
        "title": "Threats by Severity",
        "title_size": "16",
        "title_align": "left",
        "type": "toplist",
        "requests": [
          {
            "log_query": {
              "index": "*",
              "search": {
                "query": "source:pan.firewall @type:THREAT $severity $type $deviceName $destinationIp $userId $eventName $subType $countryName $clientIp"
              },
              "group_by": [
                {
                  "facet": "@severity",
                  "sort": {
                    "order": "desc",
                    "aggregation": "count"
                  },
                  "limit": 50
                }
              ],
              "compute": {
                "aggregation": "count"
              }
            }
          }
        ]
      }
