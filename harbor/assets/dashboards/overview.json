{
    "title": "Harbor Integration Dashboard",
    "description": "",
    "widgets": [
        {
            "id": 0,
            "definition": {
                "type": "image",
                "url": "/static/images/logos/harbor_large.svg",
                "sizing": "zoom"
            },
            "layout": {
                "x": 2,
                "y": 4,
                "width": 25,
                "height": 8
            }
        },
        {
            "id": 1,
            "definition": {
                "type": "check_status",
                "title": "Components health",
                "title_size": "16",
                "title_align": "center",
                "check": "harbor.status",
                "grouping": "cluster",
                "group": "$host",
                "group_by": [
                    "component"
                ],
                "tags": [
                    "$host"
                ],
                "time": {
                    "live_span": "10m"
                }
            },
            "layout": {
                "x": 2,
                "y": 26,
                "width": 25,
                "height": 9
            }
        },
        {
            "id": 2,
            "definition": {
                "type": "check_status",
                "title": "Can connect",
                "title_size": "16",
                "title_align": "center",
                "check": "harbor.can_connect",
                "grouping": "cluster",
                "group": "$host",
                "group_by": [],
                "tags": [
                    "$host"
                ]
            },
            "layout": {
                "x": 2,
                "y": 14,
                "width": 25,
                "height": 9
            }
        },
        {
            "id": 3,
            "definition": {
                "type": "timeseries",
                "requests": [
                    {
                        "q": "avg:harbor.disk.total{$host}",
                        "display_type": "area",
                        "style": {
                            "palette": "dog_classic",
                            "line_type": "solid",
                            "line_width": "normal"
                        }
                    },
                    {
                        "q": "avg:harbor.disk.free{$host}",
                        "display_type": "area",
                        "style": {
                            "palette": "dog_classic",
                            "line_type": "solid",
                            "line_width": "normal"
                        }
                    }
                ],
                "custom_links": [],
                "title": "Harbor volume metrics",
                "title_size": "16",
                "title_align": "left",
                "show_legend": false,
                "legend_size": "0"
            },
            "layout": {
                "x": 32,
                "y": 4,
                "width": 47,
                "height": 15
            }
        },
        {
            "id": 4,
            "definition": {
                "type": "query_value",
                "requests": [
                    {
                        "q": "avg:harbor.projects.count{$host}",
                        "aggregator": "last"
                    }
                ],
                "custom_links": [],
                "title": "Number of projects",
                "title_size": "16",
                "title_align": "left",
                "autoscale": true,
                "custom_unit": "proj",
                "precision": 0
            },
            "layout": {
                "x": 32,
                "y": 20,
                "width": 47,
                "height": 15
            }
        }
    ],
    "template_variables": [
        {
            "name": "host",
            "default": "*",
            "prefix": "host"
        }
    ],
    "layout_type": "free",
    "is_read_only": true,
    "notify_list": [],
    "id": 30276
}