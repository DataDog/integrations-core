    {
      "id": 1,
      "definition": {
        "title": "Basic Activity Monitor",
        "reflow_type": "fixed",
        "type": "group",
        "layout_type": "ordered",
        "widgets": [
          {
            "id": 7,
            "definition": {
              "title": "Connections to Server - Rate",
              "title_size": "16",
              "title_align": "left",
              "type": "query_value",
              "requests": [
                {
                  "q": "avg:mysql.net.connections{$scope}",
                  "aggregator": "avg"
                }
              ],
              "precision": 2
            },
            "layout": {
              "x": 0,
              "y": 0,
              "width": 2,
              "height": 2
            }
          },
          {
            "id": 8,
            "definition": {
              "title": "Failed Connects",
              "title_size": "16",
              "title_align": "left",
              "type": "query_value",
              "requests": [
                {
                  "q": "avg:mysql.net.aborted_connects{$scope}",
                  "aggregator": "avg",
                  "conditional_formats": [
                    {
                      "comparator": "<=",
                      "palette": "white_on_green",
                      "value": 0
                    },
                    {
                      "comparator": ">",
                      "palette": "white_on_red",
                      "value": 0
                    }
                  ]
                }
              ],
              "precision": 2
            },
            "layout": {
              "x": 2,
              "y": 0,
              "width": 2,
              "height": 2
            }
          },
          {
            "id": 9,
            "definition": {
              "title": "Connections - Count",
              "title_size": "16",
              "title_align": "left",
              "type": "query_value",
              "requests": [
                {
                  "q": "avg:mysql.net.max_connections{$scope}",
                  "aggregator": "avg"
                }
              ],
              "autoscale": true,
              "precision": 2
            },
            "layout": {
              "x": 4,
              "y": 0,
              "width": 2,
              "height": 2
            }
          }
        ]
      },
      "layout": {
        "x": 6,
        "y": 0,
        "width": 6,
        "height": 3
