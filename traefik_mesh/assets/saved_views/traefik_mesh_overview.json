{
    "name": "Traefik Mesh Logs Overview",
    "type": "logs",
    "page": "stream",
    "query": "source:traefik_mesh",
    "timerange": {
      "interval_ms": 3600000
    },
    "visible_facets": [
        "source",
        "host",
        "service"
    ],
    "options": {
      "columns": [
        "host",
        "service",
        "status"
      ],
      "show_date_column": true,
      "show_message_column": true,
      "message_display": "inline",
      "show_timeline": true
    }
}