      "source_type_name": "WebLogic",
      "configuration": {
        "spec": "assets/configuration/spec.yaml"
      },
      "events": {
        "creates_events": false
      },
      "metrics": {
        "prefix": "weblogic.",
        "check": "weblogic.jvm_runtime.heap_size",
