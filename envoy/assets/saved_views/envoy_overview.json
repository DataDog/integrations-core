{
    "name": "Envoy Response Logs Overview",
    "query": "source:envoy ",
    "options": {
        "patterns": {
            "aggregationQueries": [
                "status",
                "service"
            ]
        }
    },
    "visible_facets": [
        "source",
        "host",
        "service",
        "@http.status_code",
        "@http.method",
        "@http.url",
        "@http.authority"
    ],
    "type": "logs",
    "timerange": {
        "interval": 900000
    },
    "page": "patterns"
}